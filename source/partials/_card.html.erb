<script id="card-template" type="text/x-handlebars-template">
  <a class="card" style="background-image: url({{image}})"
     data-catalogue="{{cat}}"
     data-typology="{{type}}"
     data-region="{{region}}"
     data-group="{{group}}"
     data-start="{{startDate}}"
     data-end="{{endDate}}"
     href="{{url}}">
    <div class="card-title">
      <h5 class="mb0">{{cat}}. {{title}}</h5>
      <h6 class="card-date mb0">{{date}}</h6>
    </div>
  </a>
</script>
