<script id="card-template" type="text/x-handlebars-template">
  <a class="card" style="background-image: url({{image}})"
     data-catalogue="{{cat}}"
     data-typology="{{type}}"
     data-region="{{region}}"
     data-group="{{group}}"
     href="{{url}}">
    <h5 class="card-title">{{cat}}. {{title}}</h5>
  </a>
</script>
